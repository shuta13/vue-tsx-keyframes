{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint"
    ],
    "plugins": ["prettier"],
    "parser": "@typescript-eslint/parser",
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "rules": {
        "prettier/prettier": "error"
    }
}