{
  "name": "vue-tsx-keyframes",
  "version": "1.0.7",
  "description": "library of keyframes as CSS in JS for TSX on Vue",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.config.js --watch",
    "test": "jest",
    "lint": "eslint \"{src,types,test}/**/*.ts\"",
    "fix": "prettier --write \"{src,types,test}/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shuta13/vue-tsx-keyframes.git"
  },
  "keywords": [
    "Vue",
    "TypeScript",
    "TSX"
  ],
  "author": "did0es(Shuta Hirai) <hollyflatpair@outlook.jp>",
  "license": "MIT",
  "homepage": "https://github.com/shuta13/vue-tsx-keyframes#readme",
  "devDependencies": {
    "@types/jest": "24.9.0",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "prettier": "2.7.1",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "peerDependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*test.ts"
    ]
  }
}
